= javascript_include_tag 'codemirror/codemirror', 'codemirror/mirrorframe'

%h2 New App
- form_for [:developer, @app] do |form|
  = form.error_messages

  = form.hidden_field :parent_id

  %p
    = form.label :title
    = form.text_field :title

  %p
    = form.label :description
    = form.text_field :description
